#include <stdio.h>
int main() {
int n=5; 
for(int i=1; i<=2*n; i++){
for(int j=1; j<=2*n; j++){
   int RelativeRow = 2*n+1-i;
      printf((i<=5? (j<=i || j>=2*n+1-i? "*" : " ") : (j<=RelativeRow || j>=2*n+1-RelativeRow? "*" : " ") ));
}
printf("\n");
}
return 0; 
}
